<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/IFFL_Idx"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Idx"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/IFFL_SellerName"
                android:layout_width="200dip"
                android:layout_height="wrap_content"
                android:text="SellerName"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/IFFL_iff1"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="IFF1"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/IFFL_iff2"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="IFF2"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/IFFL_iff3"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="IFF3"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/IFFL_iff4"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="IFF4"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/IFFL_iff5"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="IFF5"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/IFFL_iff6"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="IFF6"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/IFFL_iff7"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="IFF7"
                android:textColor="@android:color/black" />

        </LinearLayout>
    </LinearLayout>


</LinearLayout>