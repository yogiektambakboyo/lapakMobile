<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:paddingTop="3dip"
    android:paddingBottom="3dip">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:id="@+id/LSOS_Lay"
        android:paddingLeft="3dip"
        android:paddingBottom="2dip"
        android:paddingTop="2dip"
        android:elevation="1dp">



        <LinearLayout
            android:orientation="vertical"
            android:layout_width="160dip"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:id="@+id/LSOS_ProductCode"
                android:layout_marginRight="10dip"
                android:layout_marginLeft="10dip"
                android:visibility="gone" />

            <TextView
                android:layout_width="160dip"
                android:layout_height="wrap_content"
                android:text="Nama"
                android:id="@+id/LSOS_Name"
                android:textColor="@android:color/black" />

        </LinearLayout>

        <TextView
            android:layout_width="70dip"
            android:layout_height="wrap_content"
            android:text="99"
            android:id="@+id/LSOS_Norm"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@android:color/black" />

        <TextView
            android:layout_width="70dip"
            android:layout_height="wrap_content"
            android:text="99"
            android:id="@+id/LSOS_Total"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@android:color/black" />

        <TextView
            android:layout_width="70dip"
            android:layout_height="wrap_content"
            android:text="99"
            android:id="@+id/LSOS_Target"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@android:color/black" />

        <TextView
            android:layout_width="120dip"
            android:layout_height="wrap_content"
            android:text="99"
            android:id="@+id/LSOS_Actual"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@android:color/black" />


        <TextView
            android:layout_width="70dip"
            android:layout_height="wrap_content"
            android:text="99"
            android:id="@+id/LSOS_Gap"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@android:color/black" />

        <LinearLayout
            android:layout_width="55dip"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|center_horizontal">
            <ImageView
                android:layout_height="28dip"
                android:id="@+id/LSOS_Photo"
                android:gravity="center_vertical|center_horizontal"
                android:background="@drawable/sfa_error"
                android:layout_width="28dip" />
        </LinearLayout>

        <TextView
            android:layout_width="160dip"
            android:layout_height="wrap_content"
            android:text="-"
            android:id="@+id/LSOS_Reason"
            android:gravity="center_vertical|center_horizontal"
            android:textColor="@android:color/black" />

    </LinearLayout>


</LinearLayout>