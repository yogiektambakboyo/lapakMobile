<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TabHost
        android:id="@+id/Dialog_TabHost"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TabWidget
                android:id="@android:id/tabs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <FrameLayout
                android:id="@android:id/tabcontent"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/tab1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <HorizontalScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:layout_marginBottom="10dip"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="50dip"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="Idx"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_SellerName"
                                    android:layout_width="200dip"
                                    android:layout_height="wrap_content"
                                    android:text="Store Name"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff1"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff2"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff3"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff4"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff5"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff6"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff7"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                            </LinearLayout>

                            <ListView
                                android:id="@+id/IFF_Ls"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" />

                        </LinearLayout>


                    </HorizontalScrollView>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/tab2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <HorizontalScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:layout_marginBottom="10dip"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="50dip"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="Idx"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_SellerNameX"
                                    android:layout_width="200dip"
                                    android:layout_height="wrap_content"
                                    android:text="Store Name"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff1X"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff2X"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff3X"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff4X"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff5X"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff6X"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                                <TextView
                                    android:id="@+id/IFF_iff7X"
                                    android:layout_width="100dip"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:textColor="@color/colorPrimaryDark" />

                            </LinearLayout>

                            <ListView
                                android:id="@+id/IFF_LsX"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" />

                        </LinearLayout>


                    </HorizontalScrollView>
                </LinearLayout>

            </FrameLayout>
        </LinearLayout>
    </TabHost>





</LinearLayout>